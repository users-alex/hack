FILE=dotfiles-`whoami`.tgz

export BACKUPS="
.aws
.azure
.bash_profile
.bashrc
.bash_env
.config
.composer
.cf
.gitconfig
.gitignore
.gnupg
.profile
.pyenvrc
.rclone.conf
.rvmrc
.s3cfg
.secrets
.ssh
.yubikey
.vimrc
.zshrc
.acdbackup
README.md
"

echo "creating ${FILE}"
mkdir -p ~/private
( cd ~;tar zHcf ~/private/$FILE $BACKUPS)

(cd ~/private;~/bin/enc $FILE)
